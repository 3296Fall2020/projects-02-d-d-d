<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.openjfx.PrimaryController" minWidth="900" minHeight="600">
   <children>
      <Label text="Start screen" />
      <Button fx:id="newGameButton" text="New Game" onAction="#newGame"/>
      <Button fx:id="loadGameButton" text="Load Game" onAction="#loadGame"/>
      <Button fx:id="getEventButton" text="Test Event" onAction="#getEvent"/>
      <Button fx:id="initCombatButton" text="Start Combat" onAction="#initCombat"/>
      <Button fx:id="ventureButton" text="Venture Forth" onAction="#venture"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
