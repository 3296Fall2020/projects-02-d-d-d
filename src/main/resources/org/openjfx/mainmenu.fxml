<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.openjfx.MainMenuController" minWidth="900" minHeight="600">
   <children>
      <ImageView>
         <image>
            <Image url="@images/title_banner.png" />
         </image>
      </ImageView>
      <Text text="" fx:id="characterDescriptionField" textAlignment="CENTER"/>
      <Button fx:id="loadGameButton" text="Continue" onAction="#loadGame"/>
      <Button fx:id="newGameButton" text="New Game" onAction="#newGame"/>
      <Button fx:id="logOutButton" text="Log Out" onAction="#logOut"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
