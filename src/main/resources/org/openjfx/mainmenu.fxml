<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.openjfx.MainMenuController" minWidth="900" minHeight="600">
   <children>
      <Text fx:id="titleField"><font><Font size = "40.0"></Font></font></Text>
      <Text text="" fx:id="characterDescriptionField" textAlignment="CENTER"/>
      <Button fx:id="loadGameButton" text="Continue" onAction="#loadGame"/>
      <Button fx:id="newGameButton" text="New Game" onAction="#newGame"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
