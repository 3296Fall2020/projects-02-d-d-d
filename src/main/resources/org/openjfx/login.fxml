<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.Region?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.openjfx.LoginController" minWidth="900" minHeight="600">

    <children>
        <Label text="Start screen" />
        <Label text="username"></Label>
        <TextField fx:id="username"></TextField>
        <Label text="Password"></Label>
        <PasswordField fx:id="password"></PasswordField>
        <Region minHeight="5" disable="true"></Region>

        <Button fx:id="loginButton" text="Login" onAction="#loginButton"/>
        <Button fx:id="createProfile" text="Don't have a login yet? Create a Profile!" onAction="#createProfile"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>


